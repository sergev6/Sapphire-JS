                      </section>
                  </div>
              </div>
              <!--state overview end-->


              <div class="row">


                  <div class="col-lg-8">
                      <!--custom chart start-->
                      <div class="border-head">
                          <h3>Earning Graph</h3>
                      </div>
                      <div class="custom-bar-chart">
                          <ul class="y-axis">
                              <li><span>100</span></li>
                              <li><span>80</span></li>
                              <li><span>60</span></li>
                              <li><span>40</span></li>
                              <li><span>20</span></li>
                              <li><span>0</span></li>
                          </ul>
                          <div class="bar">
                              <div class="title">JAN</div>
							  <% var x = parseInt((dashdata.graph[1]/dashdata.totalsales)*100) %>
                              <div class="value tooltips" data-original-title="<%= x %>%" data-toggle="tooltip" data-placement="top"><%= x %>%</div>
                          </div>
                          <div class="bar ">
                              <div class="title">FEB</div>
                    		  <% var x = parseInt((dashdata.graph[2]/dashdata.totalsales)*100) %>
                              <div class="value tooltips" data-original-title="<%= x %>%" data-toggle="tooltip" data-placement="top"><%= x %>%</div>
                          </div>
                          <div class="bar ">
                              <div class="title">MAR</div>
                    		  <% var x = parseInt((dashdata.graph[3]/dashdata.totalsales)*100) %>
                              <div class="value tooltips" data-original-title="<%= x %>%" data-toggle="tooltip" data-placement="top"><%= x %>%</div>
                          </div>
                          <div class="bar ">
                              <div class="title">APR</div>
                    		  <% var x = parseInt((dashdata.graph[4]/dashdata.totalsales)*100) %>
