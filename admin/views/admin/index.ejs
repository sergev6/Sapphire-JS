                    		  <% var x = parseInt((dashdata.graph[5]/dashdata.totalsales)*100) %>
                              <div class="value tooltips" data-original-title="<%= x %>%" data-toggle="tooltip" data-placement="top"><%= x %>%</div>
                          </div>
