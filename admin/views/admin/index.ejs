                          <div class="bar ">
                              <div class="title">DEC</div>
                    		  <% var x = parseInt((dashdata.graph[12]/dashdata.totalsales)*100) %>
                              <div class="value tooltips" data-original-title="<%= x %>%" data-toggle="tooltip" data-placement="top"><%= x %>%</div>
                          </div>
                      </div>
                      <!--custom chart end-->
                  </div>
                  <div class="col-lg-4">
                      <!--new earning start-->
                      <div class="panel terques-chart">
                          <div class="panel-body chart-texture">
                              <div class="chart">
                                  <div class="heading">
                                      <span>Today</span>
									   <% var x = parseInt((dashdata.todayearning/dashdata.totalsales)*100) %>
                                      <strong>$ <%= dashdata.todayearning %> | <%= x %>%</strong>
                                  </div>
                              </div>
                          </div>
                          <div class="chart-tittle">
                              <span class="title">New Earning</span>
                           </div>
                      </div>
                      <!--new earning end-->

                      <!--total earning start-->
                      <div class="panel green-chart">
                          <div class="panel-body">
                              <div class="chart">
                                  <div class="heading">
                                      <span>
									     <%= dashdata.monthname %>	
									  </span>
                        			   <% var x = parseInt((dashdata.monthearning/dashdata.totalsales)*100) %>
                                      <strong>$ <%= dashdata.monthearning %> | <%= x %>%</strong>
                                  </div>
                                  <div id="barchart"></div>
                              </div>
                          </div>
                          <div class="chart-tittle">
                              <span class="title">Total Earning</span>
                              <span class="value">$ <%= dashdata.totalsales %></span>
                          </div>
                      </div>
                      <!--total earning end-->
                  </div>
              </div>

<script>
	countUp(<%= dashdata.ucount %>);
