                          <div class="bar">
                              <div class="title">JUL</div>
                    		  <% var x = parseInt((dashdata.graph[7]/dashdata.totalsales)*100) %>
                              <div class="value tooltips" data-original-title="<%= x %>%" data-toggle="tooltip" data-placement="top"><%= x %>%</div>
                          </div>
                          <div class="bar ">
                              <div class="title">AUG</div>
                    		  <% var x = parseInt((dashdata.graph[8]/dashdata.totalsales)*100) %>
                              <div class="value tooltips" data-original-title="<%= x %>%" data-toggle="tooltip" data-placement="top"><%= x %>%</div>
