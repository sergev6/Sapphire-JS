<script>
$("#frm").validate();
function toggle_ship(id) {
	if (id==1) {
		jQuery("#sfirstname").val(jQuery("#firstname").val());
		jQuery("#slastname").val(jQuery("#lastname").val());	
		jQuery("#scompany").val(jQuery("#company").val());	
		jQuery("#semail").val(jQuery("#email").val());	
		jQuery("#saddress1").val(jQuery("#address1").val());	
		jQuery("#saddress2").val(jQuery("#address2").val());	
		jQuery("#scity").val(jQuery("#city").val());	
		jQuery("#sstate").val(jQuery("#state").val());	
		jQuery("#sstate").val(jQuery("#state").val());	
		jQuery("#szipcode").val(jQuery("#zipcode").val());	
		jQuery('#scountry option[value="'+jQuery("#country").val()+'"]').attr("selected","selected");
		jQuery("#stelephone").val(jQuery("#telephone").val());	
		jQuery("#sfax").val(jQuery("#fax").val());	
	} else {
		jQuery("#sfirstname").val("");
		jQuery("#slastname").val("");	
		jQuery("#scompany").val("");	
		jQuery("#semail").val("");	
		jQuery("#saddress1").val("");	
		jQuery("#saddress2").val("");	
		jQuery("#scity").val("");	
		jQuery("#sstate").val("");	
		jQuery("#sstate").val("");	
		jQuery("#szipcode").val("");	
		jQuery('#scountry option[value=""]').attr("selected","selected");
		jQuery("#stelephone").val("");	
		jQuery("#sfax").val("");	
	}

}

jQuery(document).ready(function() {
	var country = "<%= (data.country)?data.country:'' %>";
	var scountry = "<%= (data.scountry)?data.scountry:'' %>";
	jQuery('#country option[value="'+country+'"]').attr("selected","selected");
	jQuery('#scountry option[value="'+country+'"]').attr("selected","selected");	
});
</script>

